<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.rongyi.arounddemo.ui.MainActivity">

    <Button
        android:id="@+id/btn_click1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="50dp"
        android:text="普通跳转" />

    <Button
        android:id="@+id/btn_click4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/btn_click1"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="25dp"
        android:text="url跳转" />


    <Button
        android:id="@+id/btn_click2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/btn_click4"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="25dp"
        android:text="图片跳转" />

    <Button
        android:id="@+id/btn_click3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/btn_click2"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="25dp"
        android:text="lib跳转" />

    <Button
        android:id="@+id/btn_click5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/btn_click3"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="25dp"
        android:text="自动注入跳转" />

</RelativeLayout>
